{
  "name": "Duolingo Dark Theme",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Apply a Dark Theme to the Duolingo Website",
  "icons" : {
    "16": "icons/duolingo.png",
    "38": "icons/duolingo.png",
    "48": "icons/duolingo.png",
    "128": "icons/duolingo.png"
  },
  "web_accessible_resources": [{
		"resources": ["icons/crown.svg", "icons/crack.svg"],
		"matches": ["https://www.duolingo.com/*"]
  }],
  "content_scripts": [
    {
	  "matches": ["https://www.duolingo.com/*"],
      "css": ["src/custom.css"],
      "js": ["src/custom.js"]
    }
  ]
}